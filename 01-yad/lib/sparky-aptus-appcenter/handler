		killall -9 yad
		./bin/aptus-app $RUNGAMADV
		./aptus-appcenter gamadv01
		exit 0
	# game-arcade1
	elif [ "$CHECKGAMARC1" != "" ]; then
		RUNGAMARC1=`echo "$CMD" | sed -e 's/gamarc1_//'`
		killall -9 yad
		./bin/aptus-app $RUNGAMARC1
		./aptus-appcenter gamarc01
		exit 0
	# game-arcade2
	elif [ "$CHECKGAMARC2" != "" ]; then
